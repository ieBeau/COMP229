{
  "name": "my_portfolio",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "author": "Blessing",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start": "npm-run-all --parallel start:client start:server",
