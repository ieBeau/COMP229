    "test": "npm run test --prefix client",
    "e2e": "cypress open"
  },
  "dependencies": {
    "global": "^4.4.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cypress": "^15.6.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5"
  }
}
